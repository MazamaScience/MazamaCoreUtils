Type: Package
Package: MazamaCoreUtils
Version: 0.4.13
Title: Utility Functions for Production R Code
Authors@R: c(
    person("Jonathan", "Callahan", email="jonathan.s.callahan@gmail.com", role=c("aut","cre")),
    person("Eli", "Grosman", email="eligrosman1@gmail.com", role=c("ctb")),
    person("Spencer", "Pease", email="spencerpease618@gmail.com", role=c("ctb")),
    person("Thomas", "Bergamaschi", email="tbergama@gmail.com", role=c("ctb"))
